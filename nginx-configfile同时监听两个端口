nginx.default.conf 
    server {
        listen 80 default_server;
        server_name _;
        return 403;
    }


server {
    listen 443 ssl default_server;
    server_name _;
    return 403;
    ssl_certificate      ca/vikduo/vikduo.com.pem;
    ssl_certificate_key  ca/vikduo/vikduo.com.key;
} 

